<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//dao.impl.jdbc.mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="dao.OrderDao">

    <insert id="createOrder" parameterType="entity.UserOrder" keyProperty="id" keyColumn="id" useGeneratedKeys="true">

        insert into userorder(order_no,user_id,shopping_id,payment,payment_type,postage,status,create_time)
					 values(#{order_no},#{user_id},#{shopping_id},#{payment},#{payment_type},#{postage},#{status},now())

    </insert>
</mapper>