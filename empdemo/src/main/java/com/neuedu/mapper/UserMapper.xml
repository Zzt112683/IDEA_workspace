<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//com.neuedu.dao.impl.jdbc.mybatis.org//DTD Mapper 3.0//EN"
        "http://com.neuedu.dao.impl.jdbc.mybatis.org/dtd/com.neuedu.dao.impl.jdbc.mybatis-3-mapper.dtd">
<mapper namespace="com.neuedu.mapper.UserMapper">
    <select id="getUserByUsername" resultType="com.neuedu.entity.User">
        select * from e_user where username = #{username}
    </select>
</mapper>